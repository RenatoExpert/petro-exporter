#!/bin/bash
# author: renatoexpert renatorraraujo@gmail.com

#	Tanques
echo "Cleaning id file"
echo "53" > idcount
echo "Cleaning inserts file"
echo "USE MEDLOGIXCRM" > inserts.sql

#	Ibametro
echo "Working on Ibametro certificates"
FILES="IBAMETRO/*xls"
echo $FILES
for f in $FILES
do
	#	Convert to txt
	tf="$f.txt"
	echo "Processing $f file into $tf..."
	ssconvert $f $tf
	#	Get info
	./bot_ibametro $tf #1>> inserts.sql
	echo ---------------
done

#	Inmetro
echo "Working on Inmetro certificates"
FILES="INMETRO/*xls"
echo $FILES
for f in $FILES
do
	tf="$f.txt"
	echo "Processing $f file into $tf..."
	ssconvert $f $tf
	#	Get info
	./bot_ibametro $tf
	echo ---------------
done

rm tqs/*/*.txt

